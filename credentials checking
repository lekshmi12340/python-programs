a=0
attempt=2
while(a<3):
    a=input("Enter the username")
    b=input("Enter the password")
    if(a=="user" and b=="user1"):
        print("Username and password is matching")
        break
    elif((a!="user" and b!="user1") and attempt==2):
        print("Either username or password is incorrect")
        attempt-=1
    else:
        print("limits over")
        a+=1
